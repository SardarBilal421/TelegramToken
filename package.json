{
  "name": "willygoldkiller-bot",
  "version": "1.0.0",
  "description": "Telegram bot for monitoring crypto signals and token tracking",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "install-deps": "npm install",
    "setup": "npm install && echo 'Setup complete!'",
    "logs": "tail -f bot.log",
    "restart": "pm2 restart willygoldkiller-bot",
    "stop": "pm2 stop willygoldkiller-bot",
    "status": "pm2 status"
  },
  "dependencies": {
    "telegram": "^2.19.10",
    "input": "^1.0.1",
    "node-fetch": "^2.6.7",
    "fs": "^0.0.1-security"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "pm2": "^5.2.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "telegram",
    "bot",
    "crypto",
    "trading",
    "signals"
  ],
  "author": "WillyGoldKiller",
  "license": "MIT"
}
